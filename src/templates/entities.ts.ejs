import { Entity, PrimaryGeneratedColumn, Column } from 'typeorm';

<% if (props.architecture === 'clean-architecture') { %><% if (props.tenantid) { %>import { RelationalTenantidEntity } from '../../../../../../common/typeorm/entities/RelationalTenantidEntity';<% } else{ %>import { RelationalWithoutTenantidEntity } from '../../../../../../common/typeorm/entities/RelationalWithoutTenantidEntity';<% } %><% } else{ %><% if (props.tenantid) { %>import { RelationalTenantidEntity } from './metadata/RelationalTenantidEntity';<% } else{ %>import { RelationalWithoutTenantidEntity } from './metadata/RelationalWithoutTenantidEntity';<% } %><% } %>

@Entity('<%= props.tableName %>')
export default class <%= props.nameCamelCaseUpperFirst %> extends <% if (props.tenantid) { %>RelationalTenantidEntity<% } else{ %>RelationalWithoutTenantidEntity<% } %> {
    @PrimaryGeneratedColumn('uuid')
    id: string;
<% props.properties.original.strings.forEach((propertie) => { %>
    @Column()
    <%= propertie %>: string;
<%}); %><% props.properties.original.numbers.forEach((propertie) => { %>
    @Column()
    <%= propertie %>: number;
<% }); %>
}
