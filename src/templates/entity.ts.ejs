import {
    Entity,
    PrimaryGeneratedColumn,
    Column,
    CreateDateColumn,
    UpdateDateColumn,
} from 'typeorm';

@Entity('<%= props.tableName %>')
export default class <%= props.nameCamelCaseUpperFirst %> {
    @PrimaryGeneratedColumn('uuid')
    id: string;
<% props.properties.original.strings.forEach((propertie) => { %>
    @Column()
    <%= propertie %>: string;
<%}); %><% props.properties.original.numbers.forEach((propertie) => { %>
    @Column()
    <%= propertie %>: number;
<% }); %>
    @CreateDateColumn()
    created_at: Date;

    @UpdateDateColumn()
    updated_at: Date;
}
